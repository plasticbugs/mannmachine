      <h1><%= @channel.name %></h1>
      <% @videos.each do |video| %>
      	<% if video.converted %>
      		<div class="converted">
      	<% else %>
      		<div class="queued">
      	<% end %>
	
      		<h4><a href=<%= video.video_id %>>
      		
